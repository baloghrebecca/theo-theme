<!doctype html>
<html>

<head>
  <title>{{ page_title }}</title>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="description" content="{{ page_description | escape }}">
  <link rel="canonical" href="{{ canonical_url }}">
  <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
  <link rel="icon" type="image/png" href="https://i.ibb.co/sQ3Gcgs/favicon-Kopie.png" sizes="96x96">
  {{ content_for_header }}
  <!-- Header hook for plugins -->
  {{ 'application.scss.css' | asset_url | stylesheet_tag }}
  {{ 'header.scss.css' | asset_url | stylesheet_tag }}
</head>

<body>
  {% include 'cartPage' %}
  {% include 'header' %}

  <main role="main">
    {{ content_for_layout }}
  </main>
  {% include 'tickerPurple' %}
  {% include 'instagram' %}
  {% include 'footer' %}
</body>


{{ 'bodymovin.js' | asset_url | script_tag }}
{{ 'ScrollTrigger.min.js' | asset_url | script_tag }}
<!-- {{ "jquery-3.5.1.min.js" | asset_url | script_tag }} -->
{{ '//ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js' | script_tag }}
{{ 'api.jquery.js' | shopify_asset_url | script_tag }}
{{ 'main.js' | asset_url | script_tag }}
{{ 'gsap.min.js' | asset_url | script_tag }}

</html>  